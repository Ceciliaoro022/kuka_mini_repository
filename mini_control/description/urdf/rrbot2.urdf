  <!-- mini here -->
  <joint name="base_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="tool0"/>
    <child link="base_link" />
  </joint>
  
  <!-- Base Link -->
  <link name="base_link">
    <collision>
      <origin xyz="0 0 0.25" rpy="0 0 0"/>
      <geometry>
        <box size="0.025 0.025 0.5"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0.25" rpy="0 0 0"/>
      <geometry>
        <box size="0.025 0.025 0.5"/>
      </geometry>
    </visual>

    <inertial>
      <origin xyz="0 0 0.25" rpy="0 0 0"/>
      <mass value="0.5"/>
      <inertia
        ixx="0.01" ixy="0.0" ixz="0.0"
        iyy="0.01" iyz="0.0"
        izz="0.01"/>
    </inertial>
  </link>

  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0.025 0.48" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.8"/>
    <limit effort="100" lower="-6" upper="6" velocity="1" />
  </joint>

  <!-- Middle Link -->
  <link name="link1">
